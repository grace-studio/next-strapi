{
  "name": "@grace-studio/next-strapi",
  "version": "1.1.0",
  "description": "Middle layer to connect a Next.js application with Strapi.",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "preversion": "npm test && npm run lint",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grace-studio/next-strapi.git"
  },
  "keywords": [],
  "author": "Grace Studio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grace-studio/next-strapi/issues"
  },
  "homepage": "https://github.com/grace-studio/next-strapi#readme",
  "devDependencies": {
    "@types/jest": "29.5.2",
    "@types/node": "20.2.5",
    "@types/qs": "6.9.7",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "prettier-eslint": "15.0.1",
    "ts-jest": "29.1.0",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "axios": "1.4.0",
    "qs": "6.11.2"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  }
}
