{
  "name": "@grace-studio/next-strapi",
  "version": "1.3.0",
  "description": "Middle layer to connect a Next.js application with Strapi.",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "preversion": "npm test && npm run lint",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grace-studio/next-strapi.git"
  },
  "keywords": [],
  "author": "Grace Studio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grace-studio/next-strapi/issues"
  },
  "homepage": "https://github.com/grace-studio/next-strapi#readme",
  "devDependencies": {
    "@types/jest": "29.5.11",
    "@types/node": "20.10.7",
    "@types/qs": "6.9.11",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.2",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "prettier-eslint": "16.2.0",
    "ts-jest": "29.1.1",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "deepmerge": "4.3.1",
    "qs": "6.11.2"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "node": ">=18"
}
